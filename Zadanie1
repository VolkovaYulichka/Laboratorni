#include <stdio.h>

int main ()
{
	int a, b, c;
	
	printf("\n\nVvedite 3 chisla sherez probel: ");
	scanf("%d %d %d", &a, &b, &c );
	
	if ((a > b)&&(a > c)) {
		printf("\n samoe bolshoe chislo: %d\n\n", a);
	} else {
		if ((b > a)&&(b > c)) {
			printf("\n samoe bolshoe chislo: %d\n\n", b);
		} else {
			if ((c > a)&&(c > b)) {
				printf("\n samoe bolshoe chislo: %d\n\n", c);
			} else {}
		}
	}
	
	return 0;
}
